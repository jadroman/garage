/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "@angular/localize"
    ],
    "baseUrl": "src",
    "paths": {
      "@utils/*": [
        "app/core/utils/*"
      ],
      "@services/*": [
        "app/core/services/*"
      ],
      "@models/*": [
        "app/core/models/*"
      ]
    },
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}